                        local x, y, z = __REAPER1__.F.RandomCoords()
                        local ent = __REAPER1__.T.Freecam[__REAPER1__.A['world_rain'].sindex + 1].tablem[math.random(2, #__REAPER1__.T.Freecam[__REAPER1__.A['world_rain'].sindex + 1].tablem)].key
