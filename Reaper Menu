                    if rtype then
                        if rtype == 'checkbox' then
                            v.func = not v.func
                        elseif rtype == 'combo' then
                            v.cindex = r1
                        elseif rtype == 'comboactive' then
                            v.sindex = r2
                            if v.toggle ~= nil then
                                v.toggle = not v.toggle
                            end
                        elseif rtype == 'combotoggle' then
