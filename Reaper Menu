            __REAPER1__.N.ReaperInvoke(0x480142959D337D00, p1, veh, 180.0, 17564220)
            __REAPER1__.N.ReaperInvoke(0x239A3351AC1DA385, veh, c.x, c.y, c.z, true, true, true) end end) end
