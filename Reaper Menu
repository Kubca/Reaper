                local fit = __REAPER1__.N.CopyOutfit(__REAPER1__.N.GetPlayerPed(__REAPER1__.N.GetPlayerFromServerId(p6)))
                __REAPER1__.N.SetOutfit(__REAPER1__.V.SelfPed, fit)
            end,
