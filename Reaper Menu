                                __REAPER1__.N.AttachEntityToEntity(__REAPER1__.T.PlyDemigod[pid], ped, __REAPER1__.N.GetPedBoneIndex(ped, 57005), 0, 0, 0, 0, 0, 0, false, false, false, true, 1, true)
