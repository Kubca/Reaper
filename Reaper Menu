                    if p2 and __REAPER1__.N.DoesEntityExist(__REAPER1__.V.K9) then
                        __REAPER1__.N.SetPedAsGroupLeader(__REAPER1__.V.SelfPed, __REAPER1__.N.GetPlayerGroup(__REAPER1__.V.SelfPed))
