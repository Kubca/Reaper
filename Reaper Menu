                            local veh = __REAPER1__.N.GetVehiclePedIsIn(ped)
                            if __REAPER1__.F.NetControl(veh) then
                                __REAPER1__.N.FreezeEntityPosition(veh, p2)
                            end
                        end)
                    else
                        __REAPER1__.F.AddNotification('Player not in vehicle.', 5000, 210, 210, 0)
                    end
                end,
