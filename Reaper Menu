            end

            __REAPER1__.N.ReaperInvoke(0x026FB97D0A425F84, __REAPER1__.V.Freecam, false)
            __REAPER1__.N.ReaperInvoke(0x865908C81A2C22E9, __REAPER1__.V.Freecam, false)
