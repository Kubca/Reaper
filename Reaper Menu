                            __REAPER1__.N.DeleteEntity(v)
                        end

                        __REAPER1__.T.LagAllPlys = {}

                        __REAPER1__.F.AddNotification('Cleaned up vehicles.', 3000, 200, 200, 200)
                    end
                end,
