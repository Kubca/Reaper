            local veh = __REAPER1__.N.ReaperInvoke(0xAF35D0D2583051B0, p2, c.x, c.y, c.z - 10, __REAPER1__.N.ReaperInvoke(0xE83D4F9BA2A38914, p1, Citizen.ResultAsFloat()), true, true, Citizen.ResultAsInteger())
