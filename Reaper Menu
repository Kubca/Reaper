                        local hash = __REAPER1__.N.GetHashKey('weapon_' .. wep)
                        if __REAPER1__.T.WeaponsHash[hash] then
                            for _k,_v in ipairs(v) do
                                if _v.extra then
                                    __REAPER1__.N.GiveWeaponComponentToPed(__REAPER1__.V.SelfPed, hash, _v.extra)
                                end
                            end
                        end
                    end
                end
